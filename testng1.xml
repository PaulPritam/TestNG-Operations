<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<!--Run via groups-->
<suite name="TryoutTestNGXml">
    <groups>
        <run>
            <include name="sendKeys">
            </include>
            <exclude name="excluded"></exclude>
        </run>
    </groups>

    <test name="Test Groups">
        <classes>
            <class name="org.example.TryoutTestNGXml"/>
        </classes>
    </test>
</suite>


